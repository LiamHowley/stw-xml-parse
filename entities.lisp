(defun entity-trie (entities-list &aux (trie (make-trie)))
  (loop
    for entity-list in entities-list
    for string = (car entity-list)
    do (loop
	 for entity in (cdr entity-list)
	 do (insert-word entity trie string)))
  trie)


(defparameter *entity-trie* (entity-trie *entity-reference*))

(declaim (ftype (function (simple-string) function) get-decoder))

(defun get-decoder (seq) 
  (lambda (index)
    (declare (fixnum index)
	     (optimize (speed 3) (safety 0)))
    (case (aref seq (1+ index))
      (#\#
       (case (aref seq (+ index 2))
	 (#\x
	  (read-hex seq (+ index 3)))
	 (t
	  (read-decimal seq (+ index 2)))))
      (t
       (read-entity seq index)))))


(declaim (ftype (function (simple-string fixnum) (values (or null simple-string) (or null fixnum)))
		read-entity
		read-decimal
		read-hex))

(defun read-entity (seq index)
  (declare (optimize (speed 3) (safety 0)))
  (let ((resume-at)
	(result)
	(foundp (walk-branch *entity-trie*))
	(depth 0))
    (declare (function foundp)
	     (fixnum depth))
    (loop
      for i of-type fixnum from index below (length seq)
      for char = (the character (aref seq i))
      for next = (or (funcall foundp char)
		     resume-at)
      for value = (when next
		    (trie-leaf next))
      unless next
	do (return (setf depth i))
      when value
	do (setf result value))
    (when result
      (values (the simple-string result) depth))))


(defun read-decimal (seq index)
  (declare (optimize (speed 3) (safety 0)))
  (let* ((depth index)
	 (dec-string
	   (with-output-to-string (stream)
	     (loop
	       for i of-type fixnum from index below (length seq)
	       for char = (aref seq i)
	       for char-code = (char-code char)
	       while (<= 48 char-code 57)
	       do (write-char char stream)
	       finally (setf depth i)))))
    (declare (fixnum depth))
    (when (> depth index)
      (values 
       (awhen (validate-char (parse-integer dec-string))
	 (the character self))
       (if (char= (aref seq depth) #\;)
	   (incf depth)
	   depth)))))


(defun read-hex (seq index)
  (declare (optimize (speed 3) (safety 0)))
  (let* ((depth index)
	 (hex-string
	   (with-output-to-string (stream)
	     (loop
	       for i of-type fixnum from index below (length seq)
	       for char = (aref seq i)
	       for char-code = (char-code char)
	       while (or (<= 48 char-code 57)
			 (<= 65 char-code 70))
	       do (write-char char stream)
	       finally (setf depth i)))))
    (declare (fixnum depth))
    (when (> depth index)
      (values 
       (awhen (validate-char (parse-integer hex-string :radix 16))
	 (the character self))
       (if (char= (aref seq depth) #\;)
	   (incf depth)
	   depth)))))


(declaim (ftype (function (fixnum) (or null character)) validate-char))

(defun validate-char (char-code)
  (declare (optimize (speed 3) (safety 0)))
  (let ((allowed '((#x1 #xD7FF) (#xE000 #xFFFD) (#x10000 #x10FFFF)))
	(restricted '((#x1 #x8) (#xB #xC) (#xE #x1F) (#x7F #x84) (#x86 #x9F)
		      (#xFDD0 #xFDDF)
		      (#x10FFFE #x10FFFF) (#xFFFFE #xFFFFF) (#xEFFFE #xFFFFF)
		      (#xDFFFE #xDFFFF) (#xCFFFE #xCFFFF) (#xBFFFE #xBFFFF)
		      (#xAFFFE #xAFFFF) (#x9FFFE #x9FFFF) (#x8FFFE #x8FFFF)
		      (#x7FFFE #x7FFFF) (#x6FFFE #x6FFFF) (#x5FFFE #x5FFFF)
		      (#x4FFFE #x4FFFF) (#x3FFFE #x3FFFF) (#x2FFFE #x2FFFF)
		      (#x1FFFE #x1FFFF)))
	(invalid '(#x0 #xFFFE #xFFFF)))
    (cond ((loop
	     for (a b) in restricted
	       thereis (<= a char-code b))
	   (case *mode*
	     (:strict
	      ;; CERROR, includes the continue restart. While technically in XML 1.1
	      ;; this is not an error, it is strongly discouraged. Let the user decide
	      ;; what to do with it.
	      (restart-case
		  (restricted-xml-character-error "The character ~a is restricted." (code-char char-code))
		(remove-character (c)
		  :report "Remove character and continue"
		  nil)
		(use-value (value)
		  :report "Replace character with another character"
		  value)))
	     (:verbose
	      (restricted-xml-character-warning "The character ~a is restricted." (code-char char-code))
	      (code-char char-code))
	     (t (code-char char-code))))
	  ((loop
	     for (a b) in allowed
	       thereis (<= a char-code b))
	   (code-char char-code))
	  ((member char-code invalid :test #'eql)
	   (restart-case
	      (invalid-xml-character-error "The character ~a is restricted." (code-char char-code))
	     (remove-character (c)
	       :report "Remove character and continue"
	       nil)
	     (use-value (value)
	       :report "Replace character with another character"
	       value))))))


(declaim (inline encode-char)
	 (ftype (function (character) simple-string) encode-char))

(defvar *special-chars*
  '((#\< . "&lt;")
    (#\> . "&gt;")
    (#\& . "&amp;")
    (#\" . "&quot;")
    (#\' . "&#39;")))

(defun encode-char (char)
  (declare (optimize (speed 3) (safety 0)))
  (let ((stream (make-string-output-stream)))
    (case (the character char)
      (#\<
       (write-string "&lt;" stream))
      (#\>
       (write-string "&gt;" stream))
      (#\&
       (write-string "&amp;" stream))
      (#\"
       (write-string "&quot;" stream))
      (#\'
       (write-string "&#39;" stream))
     (t
       (write-char char stream)))
    (the simple-string (get-output-stream-string stream))))
